{# Pagina Nueva Categoria #}
{% extends 'administracion/base_admin.html.twig' %}

{# Usamos un theme para los formularios #}
{% form_theme form 'form/base_form.html.twig' %}

{# TITULO BODY#}
{% block bodytitle %}
    {% if isEdit is defined and isEdit != false %}
        Edita Categoria
    {% else %}
        Nueva Categoria
    {% endif %}
{% endblock %}

{# CONTENT #}
{% block content %}

    {{ form_start(form) }}
    {#   esto dibuja los campos #}
    {{ form_widget(form) }}
    {{ form_end(form) }}

{% endblock %}



